{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "rating": {
      "type": "decimal"
    },
    "description": {
      "type": "richtext"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "books"
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "inversedBy": "books"
    }
  }
}
